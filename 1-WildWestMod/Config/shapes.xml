<wildwestmod>

<append xpath="/shapes">

	<shape name="railingFloatLeftLadder">
		<property name="Class" value="Ladder"/>
		<property name="Shape" value="New"/>
		<property name="Model" value="railing_float_left"/>
		<property name="CustomIcon" value="shapeRailingFloatLeft"/>
		<property name="ShapeCategories" value="Stairs"/>
		<property name="MaterialHitpointMultiplier" value="1"/>
		<property name="UseGlobalUV" value="L,L,L,L,L,L"/>
		<property name="ImposterExclude" value="true"/>
		<property name="MirrorSibling" value="railingFloatRight"/>
	</shape>

	<shape name="railingFloatRightLadder">
		<property name="Class" value="Ladder"/>
		<property name="Shape" value="New"/>
		<property name="Model" value="railing_float_right"/>
		<property name="CustomIcon" value="shapeRailingFloatRight"/>
		<property name="ShapeCategories" value="Stairs"/>
		<property name="MaterialHitpointMultiplier" value="1"/>
		<property name="UseGlobalUV" value="L,L,L,L,L,L"/>
		<property name="ImposterExclude" value="true"/>
		<property name="MirrorSibling" value="railingFloatLeft"/>
	</shape>

</append>


</wildwestmod>