<configs>
	
	<append xpath="/blocks">
	
	<!-- Fog -->
	
	<block name="fogLightGrey">
		<property name="Material" value="Mmetal"/>
		<property name="CustomIcon" value="fogLightGrey"/>
		<property name="Shape" value="ModelEntity" />
	<property name="Model" value="#@modfolder:Resources/FogBlocks.unity3d?foggray" />
	</block>
	
	<block name="fogVariantHelper">
		<property name="Extends" value="fogLightGrey"/>		
		<property name="CreativeMode" value="Player"/>
		<property name="CustomIcon" value="fogLightGrey"/>
		<property name="DescriptionKey" value="blockVariantHelperGroupDesc"/>
		<property name="ItemTypeIcon" value="all_blocks"/>
		<property name="SelectAlternates" value="true"/>
		<property name="PlaceAltBlockValue" value="fogLightGrey,fogDarkGrey,fogOrange,fogGreen,fogBlue"/>
	</block>

	<block name="fogDarkGrey">
		<property name="Material" value="Mmetal"/>
		<property name="CustomIcon" value="fogDarkGrey"/>
		<property name="Shape" value="ModelEntity" />
		<property name="Model" value="#@modfolder:Resources/FogBlocks.unity3d?foggray1block" />
	</block>

	<block name="fogOrange">
		<property name="Material" value="Mmetal"/>
		<property name="CustomIcon" value="fogOrange"/>
		<property name="Shape" value="ModelEntity" />
		<property name="Model" value="#@modfolder:Resources/FogBlocks.unity3d?fogOrange" />
	</block>

	<block name="fogGreen">
		<property name="Material" value="Mmetal"/>
		<property name="CustomIcon" value="fogGreen"/>
		<property name="Shape" value="ModelEntity" />
		<property name="Model" value="#@modfolder:Resources/FogBlocks.unity3d?fogGreen" />
	</block>

	<block name="fogBlue">
		<property name="Material" value="Mmetal"/>
		<property name="CustomIcon" value="fogBlue"/>
		<property name="Shape" value="ModelEntity" />
		<property name="Model" value="#@modfolder:Resources/FogBlocks.unity3d?fogBlue" />
	</block>

	<!-- Icicles -->
	
<block name="Ice1Block">
	<property name="Extends" value="decoEntityMetalMaster"/>
    <property name="CustomIcon" value=""/>
    <property name="Model" value="#@modfolder:Resources/Ice.unity3d?Ice1Block"/>
    <property name="StabilitySupport" value="false"/>
    <property name="Material" value="MglassBulletproof"/>
    <property name="MaxDamage" value="2"/>
    <property name="Path" value="solid"/>
    <property name="Place" value="TowardsPlacerInverted"/>
	<property name="IsDecoration" value="true"/>
    <property class="RepairItems">
		<property name="resourceSnowBall" value="10"/>
	</property>
	<drop event="Harvest" name="resourceSnowBall" count="11" tag="oreWoodHarvest"/>
	<drop event="Destroy" count="0"/>
	<drop event="Fall" name="terrSnow" count="1" prob="0.25" stick_chance="1"/>
	<drop event="Fall" name="resourceSnowBall" count="22" prob="0.187" stick_chance="0"/>
    <property name="FilterTags" value="floot,fdecor,fother,ffurniture"/>	
</block>

